{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      courseData: [],\n      name: null,\n      loading: false\n    };\n  },\n  mounted() {\n    this.loadCourse();\n  },\n  methods: {\n    loadCourse() {\n      this.loading = true;\n      this.$request.get('/course/selectAll', {\n        params: {\n          name: this.name\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.courseData = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      }).catch(err => {\n        console.error('Load course error:', err);\n        this.$message.error('Failed to load courses');\n      }).finally(() => {\n        this.loading = false;\n      });\n    },\n    reset() {\n      this.name = null;\n      this.loadCourse();\n    },\n    goToCourseDetail(courseId) {\n      // 添加调试信息\n      console.log('Navigating to course detail, ID:', courseId);\n\n      // 使用编程式导航，更安全\n      this.$router.push({\n        path: '/front/courseDetail',\n        query: {\n          id: courseId\n        }\n      }).catch(err => {\n        console.error('Navigation error:', err);\n        // 如果路由跳转失败，尝试直接跳转\n        window.location.href = `/front/courseDetail?id=${courseId}`;\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","courseData","name","loading","mounted","loadCourse","methods","$request","get","params","then","res","code","$message","error","msg","catch","err","console","finally","reset","goToCourseDetail","courseId","log","$router","push","path","query","id","window","location","href"],"sources":["src/views/front/Course.vue"],"sourcesContent":["<template>\n  <div class=\"main-content\">\n    <div class=\"container\">\n      <!-- 搜索区域 -->\n      <div class=\"search-section\">\n        <div class=\"search-box\">\n          <el-input\n              placeholder=\"Enter course name to search\"\n              class=\"search-input\"\n              v-model=\"name\"\n              clearable\n              prefix-icon=\"el-icon-search\"\n          ></el-input>\n          <el-button\n              type=\"primary\"\n              class=\"search-btn\"\n              @click=\"loadCourse\"\n              :loading=\"loading\"\n          >\n            <i class=\"el-icon-search\"></i>\n            Search\n          </el-button>\n          <el-button\n              type=\"info\"\n              plain\n              class=\"reset-btn\"\n              @click=\"reset\"\n          >\n            <i class=\"el-icon-refresh\"></i>\n            Reset\n          </el-button>\n        </div>\n      </div>\n\n      <!-- 课程列表区域 -->\n      <div class=\"course-section\">\n        <div class=\"section-title\" v-if=\"courseData.length > 0\">\n          <h2>Available Courses</h2>\n          <p>{{ courseData.length }} course{{ courseData.length > 1 ? 's' : '' }} found</p>\n        </div>\n\n        <div v-if=\"loading\" class=\"loading-section\">\n          <div class=\"loading-content\">\n            <i class=\"el-icon-loading\"></i>\n            <p>Loading courses...</p>\n          </div>\n        </div>\n\n        <div v-else-if=\"courseData.length === 0\" class=\"empty-section\">\n          <i class=\"el-icon-document\"></i>\n          <p>No courses found</p>\n        </div>\n\n        <el-row :gutter=\"24\" v-else>\n          <el-col :span=\"6\" v-for=\"item in courseData\" :key=\"item.id\" class=\"course-col\">\n            <div class=\"course-card\" @click=\"goToCourseDetail(item.id)\">\n              <div class=\"course-image-container\">\n                <img :src=\"item.img\" alt=\"Course Image\" class=\"course-image\">\n                <div class=\"course-overlay\">\n                  <i class=\"el-icon-view\"></i>\n                  <span>View Details</span>\n                </div>\n              </div>\n\n              <div class=\"course-content\">\n                <div class=\"course-header\">\n                  <h3 class=\"course-title\">