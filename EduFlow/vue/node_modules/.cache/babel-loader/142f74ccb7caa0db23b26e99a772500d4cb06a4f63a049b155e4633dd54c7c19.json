{"ast":null,"code":"import E from 'wangeditor';\nexport default {\n  name: 'MenuList',\n  data() {\n    return {\n      user: {},\n      menuData: [],\n      fromVisible: false,\n      name: null,\n      viewData: null\n    };\n  },\n  mounted() {\n    this.initUser();\n    this.loadMenu();\n  },\n  methods: {\n    initUser() {\n      try {\n        const userStr = localStorage.getItem('xm-user');\n        this.user = userStr ? JSON.parse(userStr) : {};\n      } catch (error) {\n        this.user = {};\n      }\n    },\n    loadMenu() {\n      this.$request.get('/menu/selectAll', {\n        params: {\n          name: this.name\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.menuData = res.data;\n        } else {\n          this.$message.error(res.msg || 'Failed to load recipes');\n        }\n      }).catch(() => {\n        this.$message.error('Failed to load recipes');\n      });\n    },\n    viewDescri(content) {\n      this.viewData = content;\n      this.fromVisible = true;\n    },\n    reset() {\n      this.name = null;\n      this.loadMenu();\n    }\n  }\n};","map":{"version":3,"names":["E","name","data","user","menuData","fromVisible","viewData","mounted","initUser","loadMenu","methods","userStr","localStorage","getItem","JSON","parse","error","$request","get","params","then","res","code","$message","msg","catch","viewDescri","content","reset"],"sources":["src/views/front/Menu.vue"],"sourcesContent":["<template>\n  <div class=\"main-content\">\n    <div class=\"container\">\n      <!-- Page Header -->\n      <div class=\"page-header\">\n        <h1 class=\"page-title\">Healthy Recipes</h1>\n        <p class=\"page-subtitle\">Discover nutritious and delicious recipes for your fitness journey</p>\n      </div>\n\n      <!-- Search Section -->\n      <div class=\"search-section\">\n        <div class=\"search-container\">\n          <el-input\n              placeholder=\"Enter recipe name to search\"\n              class=\"search-input\"\n              v-model=\"name\"\n              clearable>\n            <i slot=\"prefix\" class=\"el-input__icon el-icon-search\"></i>\n          </el-input>\n          <el-button\n              type=\"primary\"\n              class=\"search-btn\"\n              @click=\"loadMenu\"\n              icon=\"el-icon-search\">\n            Search\n          </el-button>\n          <el-button\n              type=\"warning\"\n              plain\n              class=\"reset-btn\"\n              @click=\"reset\"\n              icon=\"el-icon-refresh-left\">\n            Reset\n          </el-button>\n        </div>\n      </div>\n\n      <!-- Recipes Grid -->\n      <div class=\"recipes-grid\">\n        <el-row :gutter=\"30\">\n          <el-col :span=\"6\" v-for=\"item in menuData\" :key=\"item.id\" class=\"recipe-col\">\n            <div class=\"recipe-card\">\n              <div class=\"image-container\">\n                <img :src=\"item.img\" alt=\"Recipe Image\" class=\"recipe-image\">\n                <div class=\"image-overlay\">\n                  <i class=\"el-icon-view recipe-icon\"></i>\n                  <span>View Recipe</span>\n                </div>\n              </div>\n\n              <div class=\"recipe-info\">\n                <h3 class=\"recipe-name\">{{ item.name }}</h3>\n                <div class=\"recipe-meta\">\n                  <i class=\"el-icon-time\"></i>\n                  <span>Published: {{ item.time }}</span>\n                </div>\n                <el-button\n                    type=\"primary\"\n                    class=\"view-btn\"\n                    @click=\"viewDescri(item.content)\"\n                    icon=\"el-icon-document\">\n                  View Recipe\n                </el-button>\n              </div>\n            </div>\n          </el-col>\n        </el-row>\n\n        <!-- Empty State -->\n        <div v-if=\"menuData.length === 0\" class=\"empty-state\">\n          <i class=\"el-icon-food\"></i>\n          <p>No recipes found</p>\n        </div>\n      </div>\n    </div>\n\n    <!-- Recipe Detail Dialog -->\n    <el-dialog\n        title=\"Recipe Details\"\n        :visible.sync=\"fromVisible\"\n        width=\"60%\"\n        :close-on-click-modal=\"false\"\n        destroy-on-close\n        class=\"custom-dialog\">\n      <div class=\"recipe-content-wrapper\">\n        <div v-html=\"viewData\" class=\"recipe-content w-e-text w-e-text-container\"></div>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport E from 'wangeditor'\n\nexport default {\n  name: 'MenuList',\n  data() {\n    return {\n      user: {},\n      menuData: [],\n      fromVisible: false,\n      name: null,\n      viewData: null,\n    }\n  },\n  mounted() {\n    this.initUser()\n    this.loadMenu()\n  },\n  methods: {\n    initUser() {\n      try {\n        const userStr = localStorage.getItem('xm-user')\n        this.user = userStr ? JSON.parse(userStr) : {}\n      } catch (error) {\n        this.user = {}\n      }\n    },\n    loadMenu() {\n      this.$request.get('/menu/selectAll', {\n        params: { name: this.name }\n      }).then(res => {\n        if (res.code === '200') {\n          this.menuData = res.data\n        } else {\n          this.$message.error(res.msg || 'Failed to load recipes')\n        }\n      }).catch(() => {\n        this.$message.error('Failed to load recipes')\n      })\n    },\n    viewDescri(content) {\n      this.viewData = content\n      this.fromVisible = true\n    },\n    reset() {\n      this.name = null\n      this.loadMenu()\n    }\n  }\n}\n</script>\n\n<style scoped>\n.main-content {\n  background: linear-gradient(135deg, #f3f0ff 0%, #e5d3f7 100%);\n  min-height: 100vh;\n  padding: 40px 0;\n}\n\n.container {\n  width: 85%;\n  max-width: 1400px;\n  margin: 0 auto;\n}\n\n.page-header {\n  text-align: center;\n  margin-bottom: 40px;\n  background: white;\n  border-radius: 20px;\n  padding: 40px;\n  box-shadow: 0 8px 32px rgba(0, 0, 0, 0.1);\